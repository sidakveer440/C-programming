#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main () {  
    
    int x, years, weeks, days, daysLeft;
    
    scanf("%d",&x);
    
    years = x/365;
    daysLeft = x%365;
    weeks = daysLeft/7;
    days = daysLeft%7;
    
    printf("Years: %d\n",years);
    printf("Weeks: %d\n",weeks);
    printf("Days: %d\n",days);    
    
    
    
    
    return 0;
}
